# Docker Environment Configuration
# Copy this to .env and update values as needed

# Database password
DB_PASSWORD=example

# Rocket secret key (generate with: openssl rand -base64 32)
ROCKET_SECRET_KEY=change-me-generate-with-openssl-rand-base64-32

# Logging level (debug, info, warn, error)
RUST_LOG=info

# Ports (optional - defaults shown)
HTTP_PORT=80
HTTPS_PORT=443
