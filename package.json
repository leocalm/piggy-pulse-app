{
  "name": "piggy-pulse",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint . --cache",
    "stylelint": "stylelint '**/*.css' --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "vitest:storybook": "vitest run --config vitest.storybook.config.ts",
    "test": "npm run typecheck && npm run prettier && npm run lint && npm run vitest && npm run build",
    "e2e": "playwright test",
    "e2e:local:mock": "E2E_TARGET=local E2E_API_MODE=mock playwright test --project=desktop-chromium --project=mobile-chromium",
    "e2e:local:real": "E2E_TARGET=local E2E_API_MODE=real playwright test --project=desktop-chromium --project=mobile-chromium",
    "e2e:docker:mock": "E2E_TARGET=docker E2E_BASE_URL=https://localhost:18443 E2E_API_MODE=mock playwright test --project=desktop-chromium --project=mobile-chromium",
    "e2e:docker:real": "E2E_TARGET=docker E2E_BASE_URL=https://localhost:18443 E2E_API_MODE=real E2E_API_URL=http://127.0.0.1:18080 playwright test --project=desktop-chromium --project=mobile-chromium",
    "e2e:docker:up": "bash scripts/test-env-up.sh",
    "e2e:docker:down": "bash scripts/test-env-down.sh",
    "e2e:docker:reset": "bash scripts/test-env-reset.sh",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test-storybook": "vitest --config vitest.storybook.config.ts",
    "install-browsers": "npx playwright install"
  },
  "dependencies": {
    "@mantine/charts": "^8.3.10",
    "@mantine/core": "^8.3.10",
    "@mantine/dates": "^8.3.10",
    "@mantine/form": "^8.3.10",
    "@mantine/hooks": "^8.3.10",
    "@mantine/notifications": "^8.3.10",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-query": "^5.90.11",
    "@vitest/browser-playwright": "^4.0.17",
    "dayjs": "^1.11.19",
    "emoji-picker-react": "^4.17.1",
    "framer-motion": "^12.34.0",
    "i18next": "^25.8.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.3",
    "react-is": "^19.2.3",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.5.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@storybook/addon-themes": "10.1.11",
    "@storybook/addon-vitest": "10.1.11",
    "@storybook/react": "10.1.11",
    "@storybook/react-vite": "10.1.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/react-is": "^19",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "^4.0.17",
    "@vitest/browser-playwright": "^4.0.17",
    "@vitest/coverage-v8": "4.0.17",
    "eslint": "^9.39.2",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^27.0.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "storybook": "10.1.11",
    "stylelint": "^16.25.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.17"
  },
  "packageManager": "yarn@4.12.0"
}
